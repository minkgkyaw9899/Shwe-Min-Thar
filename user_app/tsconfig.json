{
    "extends": "@tsconfig/react-native/tsconfig.json",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "*": ["src/*"],
            "app/*": ["src/*"],
            "actions/*": ["src/actions/*"],
            "components/*": ["src/components/*"],
            "hooks/*": ["src/hooks/*"],
            "navigation/*": ["src/navigation/*"],
            "screens/*": ["src/screens/*"],
            "test/*": ["test/*"],
            "underscore": ["lodash"]
        }
    }
}
